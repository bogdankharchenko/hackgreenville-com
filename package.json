        "laravel-vite-plugin": "^0.7.8",
        "lodash": "^4.17.15",
        "popper.js": "^1.12",
        "resolve-url-loader": "^2.3.1",
        "sass": "^1.20.1",
        "sass-loader": "^8.0.0",
        "sharrre": "^2.0.1",
        "vite": "^4.3.9",
        "vue": "^3.3.4"
    },
    "dependencies": {
        "@fullcalendar/core": "^4.4.0",
